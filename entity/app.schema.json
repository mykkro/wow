{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "title": "App",
  "properties": {
    "ownerAdminId": {
      "title": "Owner ID",
      "type": "number",
      "required": true
    },
    "created": {
      "title": "Created",
      "type": "string",
      "required": false
    },
    "title": {
      "title": "Title",
      "type": "string",
      "required": true
    },
    "description": {
      "title": "Description",
      "type": "string",
      "required": false
    },
    "tags": {
      "title": "Tags",
      "type": "array",
      "items": {
        "type": "string"
      },
      "required": false
    },
    "apptype": {
      "title": "App type",
      "type": "string",
      "required": true
    },
    "author": {
      "title": "Author",
      "type": "string",
      "required": false
    },
    "version": {
      "title": "Version",
      "type": "string",
      "required": false
    },
    "uri": {
      "title": "URI of imported App",
      "type": "string",
      "required": true,
      "format": "uri"
    },
    "autogenerated": {
      "title": "Is App autogenerated?",
      "type": "boolean",
      "required": false
    }
  }
}