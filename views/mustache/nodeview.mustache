{{#yield-title}}
{{title}}
{{/yield-title}}
<link rel="stylesheet" href="/css/admin.bundle.css" type="text/css" /> 
<script type="text/javascript" src="/js/wow-admin.js"></script>

<div class="admin">
	<h1>{{title}}</h1>
	<div class="admin-body">
	{{{view}}}
	</div>
</div>

<!-- we need this element so widgetizer does not complain... -->
<div style="display:none;">
<svg 
    version="1.1"
    xmlns="http://www.w3.org/2000/svg"
    xmlns:wow="http://example.org/wow"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    unselectable="on" 
    style="-webkit-user-select: none;"
    id="svg"
	tabindex='1'
	viewBox="0 0 960 600"
	width="100%"
	height="100%"
	preserveAspectRatio="none"
    >
	<g id="tempgroup" style="display:none;"></g>
</svg>	
</div>
	